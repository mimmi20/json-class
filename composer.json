{
  "name": "mimmi20/json-class",
  "type": "library",
  "description": "Class wrapper for daverandom/exceptional-json",
  "homepage": "https://github.com/mimmi20/json-class",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/json-class/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-json": "*"
  },
  "require-dev": {
    "mimmi20/coding-standard": "^2.5.1",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.2.0",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-phpunit": "^1.0.0",
    "phpunit/phpunit": "^9.5.10"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "JsonClass\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonClassTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/json-class/issues",
    "source": "https://github.com/mimmi20/json-class"
  }
}
