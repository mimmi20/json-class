{
  "name": "mimmi20/json-class",
  "type": "library",
  "description": "Class wrapper for daverandom/exceptional-json",
  "homepage": "https://github.com/mimmi20/json-class",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/json-class/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "ext-json": "*",
    "daverandom/exceptional-json": "^1.0.4"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.6.1",
    "ergebnis/phpstan-rules": "^0.15.1",
    "infection/infection": "^0.13.6 || ^0.16.3",
    "mimmi20/coding-standard": "~1.0.1",
    "pepakriz/phpstan-exception-rules": "^0.11.1",
    "phpstan/extension-installer": "^1.0.4",
    "phpstan/phpstan": "^0.12.36",
    "phpstan/phpstan-deprecation-rules": "^0.12.5",
    "phpstan/phpstan-phpunit": "^0.12.15",
    "phpstan/phpstan-strict-rules": "^0.12.4",
    "phpunit/phpunit": "^7.5.20 || ^8.5.5 || ^9.1.5"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "JsonClass\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonClassTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/json-class/issues",
    "source": "https://github.com/mimmi20/json-class"
  }
}
